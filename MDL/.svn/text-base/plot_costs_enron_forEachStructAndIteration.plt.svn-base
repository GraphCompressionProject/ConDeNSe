reset
#set key top right box
set terminal postscript enhanced eps 30 color size 7,4
set nokey


#########################################################
### start: edit this area

set ylabel "compression (lower is better)"
set xlabel "structure"


### end: edit this area
#########################################################

#min = 0
#max = 7000000
#set xrange [min:max]
set xrange [0:]
set yrange [-12000:2000]

#to put an empty boundary around the
#data inside an autoscaled graph.
set offset graph 0.05,0.05,0.05,0.0
#set xtics min,(max-min)/5,max
set style fill solid 0.5 #fillstyle
set tics out nomirror

# BEFORE Sandy Hook
set datafile separator "\t"

set title "Compression per structure - iteration 1"
set output 'costs_per_struct_iteration_1_enron_orderedALL.eps'
plot "./greedySelection_cost_per_struct_iteration_1_enron_orderedALL.model" using 1:3 with points notitle pt 7

